
@using EntityLayer.Concrete
@model List<Testimonial>


<section class="w3l-clients" id="clients">
    <!-- /grids -->
    <div class="cusrtomer-layout py-5">
      <div class="container py-lg-4 py-md-3 pb-lg-0">
        <div class="heading text-center mx-auto">
          <h6 class="sub-title text-center">VALUABLE COMMENTS BY OUR CUSTOMERS</h6>
          <h4 class="hny-title mb-md-5 mb-4">Reviews of our customers who have experienced our tour trips before</h4>
        </div>
        <!-- /grids -->
        <div class="testimonial-width">
          <div id="owl-demo1" class="owl-two owl-carousel owl-theme">
              @foreach(var item in Model)
                    {
                        
                    
                    <div class="item">
              <div class="testimonial-content">
                <div class="testimonial">
                  <blockquote>
                    <span class="fa fa-quote-left" aria-hidden="true"></span>
                    @item.Command
                  </blockquote>
                  <div class="testi-des">
                    <div class="test-img"><img src="@item.Image" class="img-fluid" alt="client-img">
                    </div>
                    <div class="peopl align-self">
                      <h3>@item.Client</h3>
                      <p class="indentity">Example City</p>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            }

